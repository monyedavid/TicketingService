    "scripts": {
        "start": "NODE_ENV=production server_location=testing node build/index.js",
        "start:contabo:dev": "PORT=4000 NODE_ENV=production server_location=testing node build/index.js",
        "start:dev": "NODE_ENV=dev server_location=testing nodemon --exec ts-node --files src/index.ts",
        "test": "NODE_ENV=test jest --watch --detectOpenHandles --no-cache",
        "build:server": "rimraf ./build && tsc && copyfiles -u 1 src/**/*.graphql build",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "gen-schema-types": "ts-node --files src/GraphQL/Scripts/createTypes.ts"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "resolutions": {
        "fs-capacitor": "3.0.0"
    }
